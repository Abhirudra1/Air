<% layout("/layouts/boilerplate.ejs") -%>
<body>
    <h2>All Listings</h2>
    <form action="/listings/new" method="get">
        <button>Create listing</button>
    </form>
    <ul>
        <% for(let listing of allListings) {%>
            <li>
                <a href="/listings/<%= listing._id %>"><%= listing.title %></a>
            </li>
            <br>
        <% } %>
    </ul> 
</body>